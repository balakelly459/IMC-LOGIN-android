<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.imc">

    <application
        android:allowBackup="true"
        android:icon="@drawable/plate"
        android:label="@string/app_name"
        android:roundIcon="@drawable/plate"
        android:supportsRtl="true"
        android:theme="@style/Theme.IMC">
        <activity android:name=".ui.HistoricoActivity"></activity>
        <activity android:name=".ActivityPesagem" />
        <activity android:name=".ui.DashboardActivity" />
        <activity android:name=".ui.NovoUsuarioActivity" />
        <activity android:name=".ui.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>